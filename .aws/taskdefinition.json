{
    "family": "records-task-definition",
    "containerDefinitions": [
        {
            "name": "records",
            "image": "654654501042.dkr.ecr.us-east-1.amazonaws.com/records:latest",
            "cpu": 512,
            "memory": 1024,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [
              {
                "name": "ASPNETCORE_HTTP_PORTS",
                "value": "80"
              }
            ],
            "mountPoints": [],
            "volumesFrom": []
        }
    ],
    "taskRoleArn": "arn:aws:iam::654654501042:role/LabRole",
    "executionRoleArn": "arn:aws:iam::654654501042:role/LabRole",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "512",
    "memory": "1024",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}
